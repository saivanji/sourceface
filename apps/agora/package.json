{
  "name": "agora",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "nodemon --exec 'babel-node ./server.js'",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\" && eslint \"src/**/*.js\"",
    "build": "babel ./server.js --out-dir ./build",
    "seed:save": "node ./seed save",
    "seed:restore": "node ./seed restore",
    "migrate:create": "migrate create",
    "migrate:list": "migrate list --store='./setup/migrationStore' --compiler='js:@babel/register'",
    "migrate:up": "migrate up --store='./setup/migrationStore' --compiler='js:@babel/register'",
    "migrate:down": "migrate down --store='./setup/migrationStore' --compiler='js:@babel/register'"
  },
  "dependencies": {
    "@babel/standalone": "^7.10.4",
    "@sourceface/schema": "^0.0.1",
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^6.1.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "graphql": "^15.1.0",
    "graphql-fields": "^2.0.3",
    "graphql-iso-date": "^3.6.1",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-tools": "^6.0.10",
    "graphql-type-json": "^0.3.2",
    "graphql-type-uuid": "^0.2.0",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mustache": "^4.0.1",
    "pg-promise": "^10.4.4",
    "ramda": "^0.27.0",
    "regenerator-runtime": "^0.13.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@sourceface/config": "^0.0.24",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^6.8.0",
    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "migrate": "^1.6.2",
    "nodemon": "1.18.11",
    "prettier": "^2.0.5",
    "yargs": "^16.0.3"
  },
  "scarfSettings": {
    "enabled": false
  }
}
